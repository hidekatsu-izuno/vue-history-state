{
  "name": "vue-history-state",
  "version": "0.12.0",
  "description": "History State Module for Vue3 and Nuxt3",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./module/index.js",
  "types": "./module/index.d.ts",
  "scripts": {
    "build": "tsc --build --clean && tsc -p ./tsconfig.json && tsc -p ./tsconfig.cjs.json",
    "test": "vitest --config vite.config.ts",
    "lint": "eslint",
    "version.major": "npm run build && standard-version --release-as major",
    "version.major.alpha": "npm run build && standard-version --release-as major --prerelease alpha",
    "version.minor": "npm run build && standard-version --release-as minor",
    "version.minor.alpha": "npm run build && standard-version --release-as minor --prerelease alpha",
    "version.patch": "npm run build && standard-version --release-as patch",
    "release": "git push --follow-tags origin main && npm publish",
    "release.alpha": "git push --follow-tags origin main && npm publish --tag alpha"
  },
  "files": [
    "lib/",
    "module/",
    "src/",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./module/index.d.ts",
        "default": "./module/index.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "default": "./lib/index.js"
    },
    "./nuxt": {
      "import": {
        "types": "./module/nuxt.d.ts",
        "default": "./module/nuxt.js"
      },
      "require": {
        "types": "./lib/nuxt.d.ts",
        "default": "./lib/nuxt.js"
      },
      "default": "./lib/nuxt.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hidekatsu-izuno/vue-history-state.git"
  },
  "author": "Hidekatsu Izuno <hidekatsu.izuno@gmail.com> (https://github.com/hidekatsu-izuno/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hidekatsu-izuno/vue-history-state/issues"
  },
  "homepage": "https://github.com/hidekatsu-izuno/vue-history-state#readme",
  "keywords": [
    "vue",
    "nuxt",
    "history"
  ],
  "devDependencies": {
    "@nuxt/kit": "^3.3.1",
    "@tsconfig/recommended": "^1.0.2",
    "@types/lz-string": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "dependencies": {
    "lz-string": "^1.5.0",
    "tslib": "^2.5.0"
  }
}
